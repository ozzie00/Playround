<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id            = "@+id/venue_owner_upload_info_linerlayout"
    android:layout_width  = "match_parent"
    android:layout_height = "wrap_content"
    android:orientation   = "vertical"
    android:weightSum     = "1"
    android:background    = "@android:color/white">

    <RelativeLayout android:id= "@+id/owner_info_upload_id_block"
        android:layout_width  = "match_parent"
        android:layout_height = "wrap_content"
        android:orientation   = "horizontal"
        android:padding       = "5dip"
        android:clickable     = "true"
        android:background    = "@android:color/white">

        <TextView
            android:id                = "@+id/owner_info_upload_id_name_view"
            android:layout_width      = "wrap_content"
            android:layout_height     = "wrap_content"
            android:layout_marginTop  = "8dp"
            android:layout_marginLeft = "8dp"
            android:gravity           = "left"
            android:text              = "@string/OMEPARSEVENUEOWNERIDCOPY"
            android:textAppearance    = "?android:attr/textAppearanceSmall" />

        <ImageView
            android:id                 = "@+id/owner_info_upload_id_ImageView"
            android:layout_width       = "48dp"
            android:layout_height      = "48dp"
            android:layout_marginRight = "8dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop    = "@id/owner_info_upload_id_name_view"
            android:layout_toRightOf   = "@+id/owner_info_upload_id_name_view"
            android:gravity            = "right"
            android:src                = "@drawable/ome_me_venue"/>
    </RelativeLayout>

    <View
        android:layout_width  = "fill_parent"
        android:layout_height = "5dp"
        android:background    = "#F3F3F3"/>


    <RelativeLayout android:id= "@+id/owner_info_upload_license_block"
        android:layout_width  = "match_parent"
        android:layout_height = "wrap_content"
        android:orientation   = "horizontal"
        android:padding       = "5dip"
        android:clickable     = "true"
        android:background    = "@android:color/white">

        <TextView
            android:id                = "@+id/owner_info_upload_license_name_view"
            android:layout_width      = "wrap_content"
            android:layout_height     = "wrap_content"
            android:layout_marginTop  = "8dp"
            android:layout_marginLeft = "8dp"
            android:gravity           = "left"
            android:text              = "@string/OMEPARSEVENUEOWNERLICENSECOPY"
            android:textAppearance    = "?android:attr/textAppearanceSmall" />

        <ImageView
            android:id                 = "@+id/owner_info_upload_license_ImageView"
            android:layout_width       = "48dp"
            android:layout_height      = "48dp"
            android:layout_marginRight = "8dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop    = "@id/owner_info_upload_license_name_view"
            android:layout_toRightOf   = "@+id/owner_info_upload_license_name_view"
            android:gravity            = "right"
            android:src                = "@drawable/ome_me_venue"/>
    </RelativeLayout>

    <View
        android:layout_width  = "fill_parent"
        android:layout_height = "5.0dp"
        android:background    = "#F3F3F3"/>

    <LinearLayout
        android:id            = "@+id/venue_owner_upload_info_phone_linearlayout"
        android:layout_width  = "match_parent"
        android:layout_height = "wrap_content"
        android:orientation   = "horizontal"
        android:padding       = "5dip">

        <TextView
            android:id                     = "@+id/venue_owner_upload_info_phone"
            android:layout_width           = "100dp"
            android:layout_height          = "wrap_content"
            android:text                   = "@string/OMEPARSEMEMYVENUEPHONE"
            android:layout_alignParentLeft = "true"
            android:layout_alignParentTop  = "true"
            android:layout_marginTop       = "8dp"
            android:layout_marginLeft      = "8dp" />

        <EditText
            android:id                     = "@+id/venue_owner_upload_info_phone_Text"
            android:layout_width           = "match_parent"
            android:layout_height          = "wrap_content"
            android:layout_marginLeft      = "20dp"
            android:layout_marginRight     = "8dp"
            android:layout_marginTop       = "8dp"
            android:layout_weight          = "1"
            android:textSize               = "15sp"
            android:textColor  = "@android:color/secondary_text_dark"/>

    </LinearLayout>

    <View
        android:layout_width  = "fill_parent"
        android:layout_height = "5dp"
        android:background    = "#F3F3F3"/>

    <LinearLayout
        android:id            = "@+id/venue_owner_upload_info_contact_email_linearlayout"
        android:layout_width  = "match_parent"
        android:layout_height = "wrap_content"
        android:orientation   = "horizontal"
        android:padding       = "5dip">

        <TextView
            android:id                     = "@+id/venue_owner_upload_info_contact_email"
            android:layout_width           = "100dp"
            android:layout_height          = "wrap_content"
            android:text                   = "@string/OMEPARSEVENUEOWNERCONTACTEMAIL"
            android:layout_alignParentLeft = "true"
            android:layout_alignParentTop  = "true"
            android:layout_marginTop       = "8dp"
            android:layout_marginLeft      = "8dp" />

        <EditText
            android:id                     = "@+id/venue_owner_upload_info_contact_email_Text"
            android:layout_width           = "match_parent"
            android:layout_height          = "wrap_content"
            android:layout_marginLeft      = "20dp"
            android:layout_marginRight     = "8dp"
            android:layout_marginTop       = "8dp"
            android:layout_weight          = "1"
            android:textSize               = "15sp"
            android:textColor  = "@android:color/secondary_text_dark"/>

    </LinearLayout>

